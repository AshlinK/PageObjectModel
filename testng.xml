<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Page Object Model" verbose="1">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
        <listener class-name="listeners.CustomListener"></listener>
    </listeners>
    <test name="Login Test" parallel="none">
        <classes>
            <class name="testcases.LoginTest"/>
        </classes>
    </test>

    <test name="Create Account Test Test" parallel="none">
        <classes>
            <class name="testcases.CreateAccountTest"/>
        </classes>
    </test>
</suite>